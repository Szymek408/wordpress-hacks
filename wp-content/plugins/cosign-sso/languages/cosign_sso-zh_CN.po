msgid "CoSign Login URL"
msgstr "单点登录的登录URL地址"

#: cosign-sso.php:424
msgid "CoSign Logout URL"
msgstr "单点登录的退出URL地址"

#: cosign-sso.php:429
msgid "CoSign Service Name"
msgstr "单点登录的服务名"

#: cosign-sso.php:435
msgid "User Account"
msgstr "用户帐号"

#: cosign-sso.php:438
msgid "Auto create user account"
msgstr "自动创建用户帐号"

#: cosign-sso.php:441
msgid "new user as"
msgstr "新用户角色"

#: cosign-sso.php:449 cosign-sso.php:451
msgid "&mdash; No role for this blog &mdash;"
msgstr "&mdash; 本博客中无权限 &mdash;"

#: cosign-sso.php:458
msgid "LDAP Options"
msgstr "LDAP 选项"

#: cosign-sso.php:461
